<%= form_for([@course,@quiz]) do |f| %>
  <p>
    <%= f.label :orderNum %><br>
    <%= f.text_field :orderNum%>
  </p>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :answer %><br>
    <%= f.text_area :answer %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>