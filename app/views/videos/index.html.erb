<h1>Listing Course</h1>
<table>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Intro</th>
    <th>link</th>
  </tr>
 
  <% @videos.each do |video| %>
    <tr>
      <td><%= video.id %></td>
      <td><%= video.title %></td>
      <td><%= video.description %></td>
      <td><%= link_to "Detail",course_video_path(:id =>video.id) %></td>
    </tr>
  <% end %>
</table>