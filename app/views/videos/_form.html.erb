<%= form_for([@course,@video]) do |f| %>
  <p>
    <%= f.label :orderNum %><br>
    <%= f.text_field :orderNum %>
  </p>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :img %><br>
    <%= f.text_field :img%>
  </p>
 
  <p>
    <%= f.label :url %><br>
    <%= f.text_field :url%>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>