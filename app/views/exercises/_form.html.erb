<%= form_for([@course,@exercise]) do |f| %>
  <p>
    <%= f.label :orderNum %><br>
    <%= f.text_field :orderNum %>
  </p>
  <p>
    <%= f.label :question %><br>
    <%= f.text_area :question %>
  </p>
  <p>
    <%= f.label :answer %><br>
    <%= f.text_area :answer %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>